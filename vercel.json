{
    "version": 2,
    "builds": [
        {
            "src": "backend/dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/dist/index.js"
        }
    ],
    "buildCommand": "cd backend && bun install && bun run build",
    "env": {
        "NODE_ENV": "production"
    }
}